        "body": {
          "mode": "raw",
          "raw": "{\n  \"name\": \"John Doe\",\n  \"username\": \"john\",\n  \"password\": \"password\",\n  \"address\": \"123 Main St\",\n  \"number\": 1234567890,\n  \"identityProof\": \"AADHAR1234\",\n  \"role\": \"CUSTOMER\"\n}"
